   "source": [
    "import sys\n",
    "input = sys.stdin.readline\n",
    "n, m = map(int, input().split())\n",
    "nums = list(map(int, input().split()))\n",
    "sec = [0]\n",
    "tmp = 0\n",
    "\n",
    "for i in range(n):\n",
    "    tmp += nums[i]\n",
    "    sec.append(tmp)\n",
    "    \n",
    "for i in range(m):\n",
    "    a, b = map(int, input().split())\n",
    "    print(sec[b] - sec[a-1])"
   ]
  }
